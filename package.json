{
  "name": "fe-conf",
  "version": "0.1.7",
  "description": "基础的前端项目配置生成器，适合项目初始化的时候使用，或者完善当前项目的配置。生成的配置文件只作为通用设置，具体项目仍然需要做进一步修改以适配项目需要。",
  "homepage": "https://github.com/turing-fe/config-generator#readme",
  "bugs": {
    "url": "https://github.com/turing-fe/config-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turing-fe/config-generator.git"
  },
  "license": "MIT",
  "author": "erguotou525@gmail.com",
  "main": "dist/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "fe-conf": "./bin/main.js"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "npm run build && ts-node scripts/publish.ts before",
    "postpublish": "ts-node scripts/publish.ts after",
    "lint": "eslint ./ --ext js,ts,tsx,json --quiet --fix",
    "build": "tsc",
    "start": "ts-node src/main.ts"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "fast-glob": "^3.2.5",
    "prompts": "^2.4.0",
    "shelljs": "^0.8.4",
    "fs-extra": "^9.1.0",
    "sort-package-json": "^1.49.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/ejs": "^3.0.6",
    "@types/fs-extra": "^9.0.9",
    "@types/node": "^14.14.35",
    "@types/prompts": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "devmoji": "^2.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
